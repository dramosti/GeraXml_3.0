using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;

namespace HLP.GeraXml.bel.NFe.Estrutura
{
    public class belArma
    {
        /// <summary>
        /// Indicador do tipo de arma de fogo; 0 - Uso permitido; 1- Uso restrito;
        /// </summary>
        private string _tparma;

        public string Tparma
        {
            get { return _tparma; }
            set { _tparma = value;    }
        }
        /// <summary>
        /// Número de série da arma
        /// </summary>
        private string _nserie;

        public string Nserie
        {
            get { return _nserie; }
            set { _nserie = value; }
        }
        /// <summary>
        /// Número de série do cano
        /// </summary>
        private string _ncano;

        public string Ncano
        {
            get { return _ncano; }
            set { _ncano = value; }
        }
        /// <summary>
        /// Descrição comleta da arma, compreendendo: calibre, marca, capacidade, tipo de funcionamento, comprimento e demaisl elemento que permitam a sua perfeita identificação.
        /// </summary>
        private string _desc;

        public string Desc
        {
            get { return _desc; }
            set { _desc = value; }
        }
    }
}
