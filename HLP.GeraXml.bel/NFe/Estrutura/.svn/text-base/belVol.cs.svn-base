using System;
using System.Collections.Generic;
using System.Linq;
using System.Text;

namespace HLP.GeraXml.bel.NFe.Estrutura
{
    public class belVol
    {
        /// <summary>
        /// Quantidade de volumes transpotados
        /// </summary>
        private decimal _qvol;

        public decimal Qvol
        {
            get { return _qvol; }
            set { _qvol = value; }
        }
        /// <summary>
        /// Espécie dos Volumes transportados
        /// </summary>
        private string _esp;

        public string Esp
        {
            get { return _esp; }
            set { _esp = HLP.GeraXml.Comum.Static.Util.TiraSimbolo(value, ""); }
