        /// <summary>
        /// Numeração dos volumes transportadoras
        /// </summary>
        private string _nvol = "0";

        public string Nvol
        {
            get { return _nvol; }
            set { _nvol = value; }
        }
        //Fim - Danner - o.s. 24432 - 04/05/2010

        /// <summary>
        /// Peso Líquido (em KG)
        /// </summary>
        private decimal _pesoL;

        public decimal PesoL
        {
            get { return _pesoL; }
            set { _pesoL = value; }
        }
        /// <summary>
        /// Peso Bruto (em KG)
        /// </summary>
        private decimal _pesoB;

        public decimal PesoB
